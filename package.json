{
  "name": "veterinary-management-system",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "server": "node server.js",
    "server:dev": "nodemon server.js",
    "init-db": "node src/utils/initDatabase.js",
    "seed": "node src/utils/seedData.js",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "start:dev": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "start:all": "concurrently --names \"BACKEND,FRONTEND\" --prefix-colors \"blue,green\" \"npm run server\" \"npm run dev\""
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-vue-next": "^0.294.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "pinia": "^2.1.7",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "vite": "^7.0.4"
  }
}
